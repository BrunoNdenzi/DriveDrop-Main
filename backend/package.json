{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "env:check": "node -e \"if (process.env.NODE_ENV !== 'production' && !require('fs').existsSync('.env')) { console.error('Error: .env file not found'); process.exit(1) }\"",
    "check:node": "node scripts/check-env.js",
    "test:supabase": "npm run build && node scripts/test-supabase.js",
    "predev": "npm run env:check && npm run check:node",
    "prestart": "npm run env:check",
    "secrets:rotate:dev": "echo \"This would rotate development secrets in a real application.\"",
    "verify:env": "node scripts/verify-env.js",
    "create:admin": "node scripts/create-admin-user.js",
    "setup": "npm run build && npm run create:admin && npm run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@supabase/supabase-js": "^2.50.5",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "stripe": "^18.3.0",
    "supertest": "^7.1.4",
    "twilio": "^5.7.3",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/googlemaps": "^3.43.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@routes": "dist/routes",
    "@utils": "dist/utils",
    "@middlewares": "dist/middlewares"
  }
}
