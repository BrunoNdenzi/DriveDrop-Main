# Mobile Environment Configuration (.env.example)
# IMPORTANT: Never commit actual secrets to version control
# Copy this file to .env and fill in your values

# =================================================================
# Core Configuration
# =================================================================
# API Endpoints
EXPO_PUBLIC_API_URL=http://localhost:3000/api/v1
EXPO_PUBLIC_ENV=development

# =================================================================
# Authentication & Security
# =================================================================
# Auth0 (if using Auth0 for authentication)
# EXPO_PUBLIC_AUTH0_DOMAIN=your-tenant.auth0.com
# EXPO_PUBLIC_AUTH0_CLIENT_ID=your-client-id

# Firebase (if using Firebase services)
# EXPO_PUBLIC_FIREBASE_API_KEY=your-api-key
# EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-app.firebaseapp.com
# EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
# EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-bucket.appspot.com
# EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
# EXPO_PUBLIC_FIREBASE_APP_ID=your-app-id

# =================================================================
# Maps & Location Services
# =================================================================
# Google Maps API Key (restricted to your app's bundle ID)
# EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-maps-key-with-restrictions

# =================================================================
# Analytics & Monitoring
# =================================================================
# Google Analytics Measurement ID
# EXPO_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Sentry DSN for error reporting
# EXPO_PUBLIC_SENTRY_DSN=https://your-sentry-key@sentry.io/your-project

# =================================================================
# Feature Flags & App Configuration
# =================================================================
# Feature Toggles
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false

# App Version Overrides (if needed for A/B testing)
# EXPO_PUBLIC_FORCE_APP_VERSION=1.0.0

# =================================================================
# External Services & Integrations
# =================================================================
# Stripe (if using Stripe for payments)
# EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_key

# Social Logins
# EXPO_PUBLIC_FACEBOOK_APP_ID=your-facebook-app-id
# EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-google-client-id

# =================================================================
# Supabase Configuration
# =================================================================
# Supabase project details (keep restricted to your app's bundle ID in production)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key
